{
    "name": "mouhannad/firebase",
    "description": "Laravel Package ",
    "type": "library",
    "keywords": [
        "mouhannad",
        "laravel",
        "firebase",
        "filters",
        "eloquent",
        "package"
    ],
    "homepage": "https://github.com/MohannadAbdAlrhem/firebase",
    "license": "MIT",
    "authors": [
        {
            "name": "MouhannadAbdAlrhem",
            "email": "mouhannadabdalrhem@gmail.com",
            "role": "Owner"
        }
